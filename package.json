{
    "name": "vdi",
    "private": false,
    "version": "0.4.7",
    "main": "dist/vdi.cjs.js",
    "module": "dist/vdi.es.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/vdi.es.js",
            "require": "./dist/vdi.cjs.js"
        }
    },
    "files": [
        "dist",
        "package.json"
    ],
    "author": "Jinghui_S",
    "description": "vue3 Dependency injection And some small functions",
    "scripts": {
        "dev": "vite --host",
        "build": "vite build && rimraf types && tsc --build tsconfig-build.json",
        "preview": "vite preview",
        "release": "pnpm build && npx release-it",
        "docs:dev": "vitepress dev docs --host",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs"
    },
    "homepage": "https://github.com/JinghuiS/vdi",
    "repository": {
        "type": "git",
        "url": "https://github.com/JinghuiS/vdi"
    },
    "bugs": {
        "url": "https://github.com/JinghuiS/vdi/issues",
        "email": "1292077619@qq.com"
    },
    "license": "MIT",
    "keywords": [
        "vue",
        "di",
        "vue-di"
    ],
    "release-it": {
        "git": {
            "commitMessage": "chore(release): ${version}",
            "tagName": "v${version}"
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular",
                "infile": "CHANGELOG.md",
                "ignoreRecommendedBump": true
            }
        },
        "github": {
            "release": true,
            "web": true
        }
    },
    "dependencies": {
        "@wendellhu/redi": "^0.6.0",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@release-it/conventional-changelog": "^5.0.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "prism-theme-vars": "^0.2.2",
        "release-it": "^15.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.5.4",
        "url": "^0.11.0",
        "vite": "^4.0.4",
        "vitepress": "^0.22.4",
        "vue-tsc": "^1.0.24"
    }
}
